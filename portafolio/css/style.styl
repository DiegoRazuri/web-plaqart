@font-face
  font-family 'menu'
  src url('fonts/menu.eot')
  src url('fonts/menu.eot') format('embedded-opentype'), url('fonts/menu.woff') format('woff'),	url('fonts/menu.ttf') format('truetype'),	url('fonts/menu.svg') format('svg')
  font-weight normal
  font-style normal

[class^="icon-"],
[class*=" icon-"]
  font-family: 'menu'
  speak: none
  font-style: normal
  font-weight: normal
  font-variant: normal
  text-transform: none
  line-height: 1


  -webkit-font-smoothing: antialiased
  -moz-osx-font-smoothing: grayscale

.icon-menu:before
  content: "\e600"

